{
    "name": "bittyphp/bitty-twig",
    "description": "A Twig template view for Bitty.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Kingsley",
            "email": "bizurkur@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "bittyphp/bitty-view": "^2.0",
        "twig/twig": "^1.35|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.0",
        "jakub-onderka/php-parallel-lint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Bitty\\View\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bitty\\Tests\\View\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
